{
  "manifest_version": 3,

  "name": "wikitimer",
  "description": "WikiTimer the best friend of Wiki-Speedrunner",
  "version": "0.0.1",

  "action": {
    "default_icon": "white-clock.jpg",
    "default_popup": "index.html",
    "default_title": "WikiTimer"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ]
}